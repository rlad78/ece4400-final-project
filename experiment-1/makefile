exp1: light_vehicle light_stoplight

light_vehicle: light_vehicle.c
	gcc -g -Wall -o light_vehicle light_vehicle.c

light_stoplight: light_stoplight.c
	gcc -g -Wall -o light_stoplight light_stoplight.c

light_parser: light_parser.c
	gcc -g -Wall -o light_parser light_parser.c

clean:
	rm -f light_vehicle light_stoplight

