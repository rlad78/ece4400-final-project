exp2: city_vehicle city_server server_analysis

city_vehicle: city_vehicle.c
	gcc -g -Wall -o city_vehicle city_vehicle.c

city_server: city_server.c
	gcc -g -Wall -o city_server city_server.c

server_analysis: server_analysis.c
	gcc -g -Wall -o server_analysis server_analysis.c

clean:
	rm -f city_vehicle city_server server_analysis

